{
    "name": "expiry-checker",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "build:babel": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
        "build:upload:sourcemaps": "npx sentry-cli sourcemaps inject ./dist && npx sentry-cli sourcemaps upload ./dist",
        "dev": "yarn dev:server && yarn dev:queue",
        "dev:server": "ts-node-dev --inspect -r dotenv/config -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
        "dev:queue": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/Queue.ts",
        "typeorm": "ts-node -r dotenv/config -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/Services/TypeORM.ts",
        "test": "jest",
        "test:watch": "jest --watch --verbose=true"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.735.0",
        "@aws-sdk/lib-storage": "^3.735.0",
        "@aws-sdk/s3-request-presigner": "^3.735.0",
        "@sentry/node": "^8.52.0",
        "@sentry/profiling-node": "^8.52.0",
        "axios": "^1.7.9",
        "bcrypt": "^5.1.1",
        "bull": "^4.16.5",
        "cors": "^2.8.5",
        "crypto-js": "^4.2.0",
        "date-fns": "^2.30.0",
        "date-fns-tz": "^1.3.8",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-async-errors": "^3.1.1",
        "firebase-admin": "^13.0.2",
        "ioredis": "5.4.2",
        "multer": "^1.4.5-lts.1",
        "node-schedule": "^2.0.0",
        "pg": "^8.13.1",
        "rate-limiter-flexible": "^2.4.2",
        "reflect-metadata": "^0.2.2",
        "sharp": "^0.32.6",
        "stream-json": "^1.7.4",
        "typeorm": "0.3.20",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@babel/cli": "^7.26.4",
        "@babel/core": "^7.26.7",
        "@babel/node": "^7.26.0",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.25.9",
        "@babel/preset-env": "^7.26.7",
        "@babel/preset-typescript": "^7.26.0",
        "@sentry/cli": "^2.41.1",
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.10",
        "@types/crypto-js": "^4.0.1",
        "@types/dotenv": "^8.2.3",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.14",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.12.0",
        "@types/node-schedule": "^1.3.2",
        "@types/stream-json": "^1.7.2",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^4.11.1",
        "@typescript-eslint/parser": "^4.11.1",
        "babel-core": "^6.26.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-plugin-transform-typescript-metadata": "^0.3.2",
        "eslint": "^7.17.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^7.1.0",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.3.0",
        "jest": "^29.7.0",
        "prettier": "^2.2.1",
        "supertest": "^6.3.4",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.7.3"
    },
    "packageManager": "yarn@4.6.0"
}
