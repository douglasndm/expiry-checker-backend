{
	"name": "expiry-checker",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files && yarn sentry:sourcemaps",
		"build:no-sourcemaps": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
		"dev": "ts-node-dev --inspect -r dotenv/config -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
		"dev:queue": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/Queue.ts",
		"db:migrate:create": "npx typeorm migration:create",
		"db:migrate": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:run -d ./src/Services/TypeORM.ts",
		"db:migrate:show": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:show -d ./src/Services/TypeORM.ts",
		"admin": "ts-node-dev --inspect -r dotenv/config -r tsconfig-paths/register --respawn --transpile-only src/Admin/server.ts",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --watch",
		"test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --coverage",
		"sentry:sourcemaps": "sentry-cli sourcemaps inject --org varlabs --project expiry-checker-backend ./dist && sentry-cli --url https://exceptions.controledevalidades.com/ sourcemaps upload --org varlabs --project expiry-checker-backend ./dist"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.741.0",
		"@aws-sdk/lib-storage": "^3.741.0",
		"@aws-sdk/s3-request-presigner": "^3.741.0",
		"@sentry/node": "^9.5.0",
		"@sentry/profiling-node": "^9.5.0",
		"axios": "^1.7.9",
		"bcrypt": "^5.1.1",
		"bull": "^4.16.5",
		"cors": "^2.8.5",
		"date-fns": "^4.1.0",
		"date-fns-tz": "^3.2.0",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"express-async-errors": "^3.1.1",
		"firebase-admin": "^13.0.2",
		"ioredis": "5.4.2",
		"multer": "^1.4.5-lts.1",
		"node-schedule": "^2.1.1",
		"pg": "^8.13.1",
		"rate-limiter-flexible": "^5.0.5",
		"reflect-metadata": "^0.2.2",
		"sharp": "^0.32.6",
		"stripe": "^17.7.0",
		"typeorm": "0.3.21",
		"yup": "^1.6.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.26.4",
		"@babel/core": "^7.26.7",
		"@babel/node": "^7.26.0",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-decorators": "^7.25.9",
		"@babel/preset-env": "^7.26.7",
		"@babel/preset-typescript": "^7.26.0",
		"@eslint/js": "^9.19.0",
		"@jest/globals": "^29.7.0",
		"@sentry/cli": "^2.42.4",
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.17",
		"@types/dotenv": "^8.2.3",
		"@types/express": "4.17.21",
		"@types/jest": "^29.5.14",
		"@types/multer": "^1.4.12",
		"@types/node": "^22.13.1",
		"@types/node-schedule": "^1.3.2",
		"@types/supertest": "^6.0.2",
		"babel-core": "^6.26.3",
		"babel-plugin-module-resolver": "^5.0.2",
		"babel-plugin-transform-typescript-metadata": "^0.3.2",
		"eslint": "^9.19.0",
		"eslint-config-prettier": "^10.0.1",
		"globals": "^15.14.0",
		"jest": "^29.7.0",
		"prettier": "3.4.2",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.23.0"
	},
	"packageManager": "yarn@4.7.0"
}
